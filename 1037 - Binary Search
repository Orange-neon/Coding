#include<bits/stdc++.h>
using namespace std;
long long findB(vector<long long>& nums, long long target, long long n) {
    long long i = 0, j = n - 1;
    while (i <= j) {
        long long middleI = (i + j) / 2;
        if (nums[middleI] < target) {
            i = middleI + 1;
        } else if (nums[middleI] > target) {
            j = middleI - 1;
        } else {
            return middleI;
        }
    }
    return -1;
}
long long findEarlyO(vector<long long>& nums, long long target, long long targetI) {
    while (nums[targetI - 1] == target && targetI != 0) {
        --targetI;
    }
    return targetI;
}
int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);
    //freopen("1.in", "r", stdin);
    long long n;
    cin >> n;
    vector<long long> nums;
    for (long long i = 0; i < n; ++i) {
        long long x;
        cin >> x;
        nums.push_back(x);
    }
    sort(nums.begin(), nums.end());
    long long target;
    while (cin >> target) {
        long long loc = findB(nums, target, n);
        if (loc != 0 && loc != -1) {
            loc = findEarlyO(nums, target, loc);
        }
        cout << loc + 1 << " ";
    }
}
