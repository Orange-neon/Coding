#include <bits/stdc++.h>
using namespace std;
int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);
    //freopen("1.in", "r", stdin);
    int n, k, ans = 0;
    cin >> n >> k;
    const int barnS = 15;
    vector<vector<int>> barn(barnS, vector<int>(barnS, 0));
    vector<vector<int>> barn_p(barnS, vector<int>(barnS, 0));
    for (int i = 0; i < n; ++i) {
        int x1, y1, x2, y2;
        cin >> x1 >> y1 >> x2 >> y2;
        ++x1;
        ++x2;
        ++y1;
        ++y2;
        barn[y1][x1] += 1;
        barn[y1][x2] += -1;
        barn[y2][x1] += -1;
        barn[y2][x2] += 1;
    }
    for (int j = 1; j < barnS; ++j) {
        for (int i = 1; i < barnS; ++i) {
            barn_p[j][i] = barn[j][i] + barn_p[j - 1][i] + barn_p[j][i - 1] - barn_p[j - 1][i - 1];
        }
    }
    for (int j = 0; j < barnS; ++j) {
        for (int i = 0; i < barnS; ++i) {
            if (barn_p[j][i] == k) {
                ++ans;
            }
        }
    }
    cout << ans;
}