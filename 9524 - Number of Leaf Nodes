#include <bits/stdc++.h>
using namespace std;
int n;
vector<vector<int>> connections;
set<int> leaves;
void dfs(int node) {
    bool extended = false;
    for (int i = 0; i < n; ++i) {
        if (connections[node][i]) {
            extended = true;
            leaves.erase(node);
            connections[node][i] = 0;
            connections[i][node] = 0;
            //cout << "\n I extended from "<<node<<" to " << i<<'\n';
            dfs(i);
        }
    }
}
int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);
    cin >> n;
    connections.resize(n, vector<int>(n,0));
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < n; ++j) {
            char pt;
            cin >> pt;
            if (pt == '1') {
                connections[j][i] = 1;
            } else {
                connections[j][i] = 0;
            }
        }
    }
    /*
    cout << "\n HEYYY THIE IS WHNAT IM GETTINGGGG\n";
    for (auto i : connections) {
        for (int j : i) {
            cout << j;
        }
        cout << '\n';
    }
    */ //input is right
    for (int i = 0; i < n; ++i) {
        leaves.insert(i);
    }
    dfs(0);
    cout << leaves.size() <<'\n';
    for (int i : leaves) {
        cout << i + 1 << " ";
    }
}