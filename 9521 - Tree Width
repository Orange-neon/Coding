#include <bits/stdc++.h>
using namespace std;
int n;
vector<int> max_width;
vector<vector<int>> connections;
void dfs(int cdepth, int node) {
    ++max_width[cdepth];
    for (int i = 0; i < n; ++i) {
        if (connections[node][i] == 1) {
            connections[node][i] = 0;
            connections[i][node] = 0;
            dfs(cdepth+1, i);
        }
    }
}
int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);
    cin >> n;
    max_width.resize(n, 0);
    connections.resize(n, vector<int>(n,0));
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < n; ++j) {
            char pt;
            cin >> pt;
            if (pt == '1') {
                connections[i][j] = 1;
            } else {
                connections[i][j] = 0;
            }
        }
    }
    /*
    cout << "\n HEYYY THIE IS WHNAT IM GETTINGGGG\n";
    for (auto i : connections) {
        for (int j : i) {
            cout << j;
        }
        cout << '\n';
    }
    */ //input is right
    dfs(0, 0);
    cout << *max_element(max_width.begin(), max_width.end());
}