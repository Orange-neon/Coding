#include <bits/stdc++.h>
using namespace std;
int n, m, sx, sy, syt, sxt;
bool madeit = false;
vector<vector<int>> visited;
vector<vector<int>> dist;
int dx[8] = {2, 1, -1, -2, -2, -1, 1, 2};
int dy[8] = {1, 2, 2, 1, -1, -2, -2, -1};

bool is_valid(int x, int y) {
    return x>=0 && x<m && y>=0 && y<n && !visited[y][x];
}

void bfs() {
    queue<pair<int, int>> q;
    q.push({sx, sy});
    visited[sy][sx] = 1;
    while (!q.empty()) {
        auto head = q.front();
        q.pop();
        for (int i = 0; i < 8; ++i) {
            int nx = head.first + dx[i], ny = head.second + dy[i];
            if (is_valid(nx, ny)) {
                q.push({nx, ny});
                visited[ny][nx] = 1;
                dist[ny][nx] = dist[head.second][head.first] +1;
            }
        }
    }
}
int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);
    cin >> n >> m >> sxt >> syt;
    sx = sxt - 1;
    sy = syt - 1;
    visited.resize(n, vector<int>(m, 0));
    dist.resize(n, vector<int>(m, 0));
    bfs();
    for (int i = 0; i < n; ++i) {
        for (int j =0; j < m; ++j) {
            if (dist[i][j] == 0) {
                if (j == sx && i == sy) {
                    cout << 0 <<"     ";
                } else {
                    cout << -1 <<"     ";
                }
            } else {
                cout << dist[i][j] <<"     ";
            }
        }
        cout <<'\n';
    }
}