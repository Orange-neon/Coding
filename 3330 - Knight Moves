#include <bits/stdc++.h>
using namespace std;
int n, sx, sy, ex, ey, ans;
bool madeit = false;
vector<vector<int>> visited;
vector<vector<int>> dist;
int dx[8] = {2, 1, -1, -2, -2, -1, 1, 2};
int dy[8] = {1, 2, 2, 1, -1, -2, -2, -1};

bool is_valid(int x, int y) {
    return x>=0 && x<n && y>=0 && y<n && !visited[y][x];
}

void bfs() {
    queue<pair<int, int>> q;
    q.push({sx, sy});
    visited[sy][sx] = 1;
    while (!q.empty()) {
        auto head = q.front();
        q.pop();
        if (head.first == ex && head.second == ey) {
            madeit = true;
            ans = dist[head.second][head.first];
        }
        for (int i = 0; i < 8; ++i) {
            int nx = head.first + dx[i], ny = head.second + dy[i];
            if (is_valid(nx, ny)) {
                q.push({nx, ny});
                visited[ny][nx] = 1;
                dist[ny][nx] = dist[head.second][head.first] +1;
            }
        }
    }
}
int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);
    cin >> n >> sx >> sy >> ex >> ey;
    visited.resize(n, vector<int>(n, 0));
    dist.resize(n, vector<int>(n, 0));
    bfs();
    if (madeit) {
        cout <<ans;
    }
}