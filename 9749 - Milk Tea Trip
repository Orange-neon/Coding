#include <bits/stdc++.h>
using namespace std;
int sx, sy, ex, ey, bobax, bobay;
int n, m, ans = 0;
bool smade = false, emade = false;
vector<vector<char>> maze;
vector<vector<int>> dist;
vector<vector<int>> visited;
vector<int> dx = {0, 0, -1, 1};
vector<int> dy = {1, -1, 0, 0};

bool is_valid(int x, int y) {
    return x>=0 && x < m && y>=0 && y < n && !visited[y][x] && !(maze[y][x]=='#') //DONT FORGET PARATHENEHTISES ();
}

void bfs() {
    queue<pair<int, int>> q;
    q.push({bobax, bobay});
    visited[bobay][bobax] = 1;
    
    while (!q.empty()) {
        auto head = q.front();
        if (head.first == sx && head.second == sy) {
            smade = true;
        } else if (head.first == ex && head.second == ey) {
            emade = true;
        }
        if (smade && emade) {
            ans = dist[sy][sx] + dist[ey][ex];
            return;
        }
        for (int i = 0; i < 4; ++i) {
            int nx = head.first + dx[i], ny = head.second + dy[i];
            if (is_valid(nx, ny)) {
                q.push({nx, ny});
                //cout << "pushed " << nx <<", " << ny <<'\n';
                visited[ny][nx] = 1;
                dist[ny][nx] = dist[head.second][head.first] + 1;
            }
        }
        q.pop();
    }
    
}
int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);
    cin >> n >> m;
    for (int i = 0; i < n; ++i) {
        vector<char> row;
        for (int j = 0; j < m; ++j) {
            char pt;
            cin >> pt;
            if (pt == 'Y') {
                sx = j;
                sy = i;
            } else if (pt == 'H') {
                ex = j;
                ey = i;
            } else if (pt == 'Q') {
                bobax = j;
                bobay = i;
            }
            row.push_back(pt);
        }
        maze.push_back(row);
    }
    //confirmed, maze does contain everything
    //  confirmed, bobax and bobay are right
    dist.resize(n, vector<int> (m, 0));
    visited.resize(n, vector<int> (m, 0));
    bfs();
    if (smade && emade) {
        cout << ans;
    } else {
        cout <<"no way";
    }
}