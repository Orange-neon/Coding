#include <bits/stdc++.h>
using namespace std;
/*
input:

6
2 1
2 3
2 4
2 5
6 5



this test case weird

10
1 2
2 4
3 1
5 3
3 6
10 3
6 7
7 8
9 7

*/
int n;
vector<vector<int>> connections;
vector<int> visited;
set<pair<int,int>> leaves;
void dfs(int node) {
    for (int i = 0; i < connections[node].size(); ++i) {
        if (!visited[connections[node][i]]) {
            visited[connections[node][i]] = 1;
            leaves.insert({connections[node][i], node});
            //cout << "\n I extended from "<<node +1<<" to " << connections[node][i] +1<<'\n';
            dfs(connections[node][i]);
        }
    }
}
int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);
    cin >> n;
    connections.resize(n, vector<int>(0,0));
    visited.resize(n, 0);
    for (int i = 0; i < n -1; ++i) {
        int x, y;
        cin >> x >> y;
        connections[x-1].push_back(y-1);
        connections[y-1].push_back(x-1);
    }
    /*
    cout << "\n HEYYY THIE IS WHNAT IM GETTINGGGG\n";
    for (auto i : connections) {
        for (int j : i) {
            cout << j;
        }
        cout << '\n';
    }
    */ //input is right
    visited[0] = 1;
    dfs(0);
    for (auto i : leaves) {
        cout << i.first+1 << ", "<<i.second + 1 << '\n';
    }
}