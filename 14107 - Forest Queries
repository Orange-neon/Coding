#include<bits/stdc++.h>
using namespace std;
int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);
    //freopen("1.in", "r", stdin);
    int n, q;
    cin >> n >> q;
    vector<vector<int>> psum(n + 1, vector<int>(n + 1, 0));
    vector<vector<int>> nums;
    for (int i = 0; i < n; ++i) {
        vector<int> row;
        for (int j = 0; j < n; ++j) {
            char x;
            cin >> x;
            if (x == '.') {
                row.push_back(0);
            } else {
                row.push_back(1);
            }
        }
        nums.push_back(row);
    }
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < n; ++j) {
            psum[i + 1][j + 1] = nums[i][j] + psum[i][j + 1] + psum[i + 1][j] - psum[i][j]; 
        }
    }
    for (int i = 0; i < q; ++i) {
        int y1, x1, y2, x2;
        cin >> y1 >> x1 >> y2 >> x2;
        cout << psum[y2][x2] - psum[y1 - 1][x2] - psum[y2][x1 - 1] + psum[y1 - 1][x1 - 1] << '\n';
    }
}