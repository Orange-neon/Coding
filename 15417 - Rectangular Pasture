/*


███╗░░░███╗██╗░░░██╗░██████╗████████╗  ███████╗██╗██╗░░██╗
████╗░████║██║░░░██║██╔════╝╚══██╔══╝  ██╔════╝██║╚██╗██╔╝
██╔████╔██║██║░░░██║╚█████╗░░░░██║░░░  █████╗░░██║░╚███╔╝░
██║╚██╔╝██║██║░░░██║░╚═══██╗░░░██║░░░  ██╔══╝░░██║░██╔██╗░
██║░╚═╝░██║╚██████╔╝██████╔╝░░░██║░░░  ██║░░░░░██║██╔╝╚██╗
╚═╝░░░░░╚═╝░╚═════╝░╚═════╝░░░░╚═╝░░░  ╚═╝░░░░░╚═╝╚═╝░░╚═╝

*/
#include<bits/stdc++.h>
using namespace std;
vector<pair<long long, long long>> Coord_Reduce(vector<pair<long long, long long>> Opast) {
    sort(Opast.begin(), Opast.end());
    for (long long i = 0; i < Opast.size(); ++i) {
        Opast[i].first = i;
    }
    vector<pair<long long, long long>> yxPast;
    for (const auto p : Opast) {
        yxPast.push_back(make_pair(p.second, p.first));
    }
    sort(yxPast.begin(), yxPast.end());
    for (long long i = 0; i < Opast.size(); ++i) {
        yxPast[i].first = i;
    }
    vector<pair<long long, long long>> rtn;
    for (const auto p : yxPast) {
        rtn.push_back(make_pair(p.second, p.first));
    }
    sort(rtn.begin(), rtn.end());
    return rtn;
}
int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);
    freopen("1.in", "r", stdin);
    long long n, ans = 0;
    cin >> n;
    vector<pair<long long, long long>> Ofield;
    for (long long i = 0; i < n; ++i) {
        long long x, y;
        cin >> x >> y;
        Ofield.push_back(make_pair(x, y));
    }
    vector<pair<long long, long long>> cows = Coord_Reduce(Ofield);
    vector<vector<long long>> pasture(n, vector<long long>(n, 0));
    vector<vector<long long>> pastureP(n + 1, vector<long long>(n + 1, 0));
    for (const auto p : cows) {
        pasture[p.second][p.first]++;
    }
    /*
    for (long long y = 0; y < n; ++y) {
        for (long long x = 0; x < n; ++x) {
            cout << pasture[y][x] << " ";
        }
        cout << '\n';
    }
    */
    for (long long j = 0; j < n; ++j) {
        for (long long i = 0; i < n; ++i) {
            pastureP[j + 1][i + 1] = pasture[j][i] + pastureP[j][i + 1] + pastureP[j + 1][i] - pastureP[j][i];
        }
    }

    for (long long i = 0; i < n - 1; ++i) {
        for (long long j = i + 1; j < n; ++j) {
            auto cow1 = cows[j], cow2 = cows[i];
            ans += pastureP[cow2.second + 1][cow2.first + 1];
            ans += pastureP[cow2.second + 1][pastureP.size() - 1] - pastureP[cow2.second + 1][cow1.first + 1] - pastureP[cow1.second][pastureP.size() - 1] + pastureP[cow1.second][cow1.first + 1];
        }
    }
    /*
    for (long long y = 0; y <= n; ++y) {
        for (long long x = 0; x <= n; ++x) {
            cout << pastureP[y][x] << " ";
        }
        cout << '\n';
    }

    cout << "\n <! ============================================> \n";
    */
    cout << ans + n + 1;
}