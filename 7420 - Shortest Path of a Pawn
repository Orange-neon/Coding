#include<bits/stdc++.h>
using namespace std;
vector<vector<int>> visited;
vector<vector<int>> dist;
int n, ans = 0, sx, sy, ex, ey;
array<int, 8> dx = {0, 0, 0, 0, 1, -1, 2, -2};
array<int, 8> dy = {1, -1, -2, 2, 0, 0, 0, 0};
bool is_valid(int x, int y) {
    return x>=0 && x < n && y>=0 && y<n && !visited[y][x];
}
void bfs() {
    queue<pair<int, int>> q;
    q.push({sx, sy});
    while (!q.empty()) {
        auto head = q.front();
        if (head.first == ex && head.second == ey) {
            ans = dist[head.second][head.first];
            return;
        }
        q.pop();
        for (int i = 0; i < 8; ++i) {
            int nx = head.first + dx[i], ny = head.second + dy[i];
            if (is_valid(nx, ny)) {
                q.push({nx, ny});
                visited[ny][nx] = 1;
                dist[ny][nx] = dist[head.second][head.first] + 1;
            }
        }
    }
}
int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);
    cin >> n >> sx >> sy >> ex >> ey;
    visited.resize(n, vector<int>(n, 0));
    dist.resize(n, vector<int>(n, 0));
    bfs();
    cout << ans;
}